class Solution {
    int k = 0;
    public int removeElement(int[] nums, int val) {
        for(int i=0; i<nums.length; i++){
            if(nums[i] == val)
                nums[i] = '_'; 
        }
    Arrays.sort(nums);
    for(int i=0; i<nums.length; i++){
        if(nums[i] != '_')
            k++;
    }
    return k;   
}
}
